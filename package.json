{
  "name": "leboncoin-exporter-ext",
  "version": "0.0.1",
  "description": "leboncoin exporter web-extension",
  "license": "MIT",
  "author": "Bamdad Sabbagh <hello@bamdadsabbagh.com>",
  "scripts": {
    "build": "webextension-toolbox build",
    "dev:chrome": "concurrently 'yarn webpack chrome' 'yarn wait:chrome && yarn start:chrome'",
    "dev:firefox": "concurrently 'yarn webpack firefox' 'yarn wait:firefox && yarn start:firefox'",
    "sort:package": "npx sort-package-json",
    "start:chrome": "web-ext run -t chromium --source-dir dist/chrome",
    "start:firefox": "web-ext run --source-dir dist/firefox",
    "wait:chrome": "wait-on dist/chrome/manifest.json",
    "wait:firefox": "wait-on dist/firefox/manifest.json",
    "webpack": "webextension-toolbox dev"
  },
  "dependencies": {
    "jspdf": "^1.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@bamdadsabbagh/eslint-config": "^1.3.3",
    "@bamdadsabbagh/stylelint-config": "^1.2.15",
    "babel-loader": "^8.0.5",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "wait-on": "^5.1.0",
    "web-ext": "^4.3.0",
    "webextension-toolbox": "^3.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
