{
  "name": "leboncoin-pdf-ext",
  "version": "0.0.1",
  "description": "PDF exporter for leboncoin.fr",
  "license": "MIT",
  "author": "Bamdad Sabbagh <hello@bamdadsabbagh.com>",
  "scripts": {
    "clean": "rm -rf dist",
    "ext:chrome": "web-ext run --source-dir dist -t chromium",
    "ext:firefox": "web-ext run --source-dir dist",
    "sort:package": "npx sort-package-json",
    "dev:chrome": "yarn clean && concurrently 'yarn webpack' 'wait-on dist/manifest.json && yarn ext:chrome'",
    "dev:firefox": "yarn clean && concurrently 'yarn webpack' 'wait-on dist/manifest.json && yarn ext:firefox'",
    "webpack": "webpack"
  },
  "dependencies": {
    "jspdf": "^1.5.3"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^1.3.5",
    "@bamdadsabbagh/stylelint-config": "^1.2.17",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "wait-on": "^5.1.0",
    "web-ext": "^4.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
